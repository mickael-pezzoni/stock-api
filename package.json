{
    "name": "stock-api",
    "version": "0.0.1",
    "description": "https://documenter.getpostman.com/view/5306761/UVsJvmXZ",
    "main": "dist/main.js",
    "scripts": {
        "build": "rimraf dist && tsc",
        "start": "cross-env NODE_ENV=development ts-node src/check-connection && ts-node-dev --respawn -- src/main.ts",
        "lint": "eslint src --ext .ts",
        "doc": "typedoc src/main.ts",
        "prettier": "prettier --write ./src",
        "prepublish": "npm run build",
        "pre-commit": "npm run lint && npm run prettier",
        "docker:build": "docker-compose --env-file .env.development -f docker-compose.yaml build",
        "docker": "docker-compose --env-file .env.development -f docker-compose.yaml up",
        "docker:db": "docker-compose --env-file .env.development -f docker-compose.yaml up -d db ",
        "prepare": "husky install"
    },
    "author": "Mickael Pezzoni",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.0.1",
        "class-validator": "^0.13.2",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.0",
        "jsonwebtoken": "^8.5.1",
        "mysql": "^2.18.1",
        "reflect-metadata": "^0.1.13",
        "speedily-js": "^0.0.6",
        "typeorm": "^0.2.45",
        "typeorm-snake-naming-strategy": "^1.0.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.16",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.4",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "ts-node-dev": "^1.1.8",
        "typedoc": "^0.22.12",
        "typescript": "^4.5.5"
    }
}
